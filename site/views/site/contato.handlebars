<section class="contact">
            <div class="max-width">
                <h2 class="title">{{contact.titleContact}}</h2>
                <div class="contact-content">
                    <div class="column left">
                        <p>{{contact.SubTitleContact}}</p>
                        <div class="icons">
                            <div class="row">
                                <i class="{{contact.iconCompany}}"></i>
                                <div class="info">
                                    <div class="head">
                                        {{contact.titleCompany}}
                                    </div>
                                    <div class="sub-title">
                                        {{contact.descCompany}}
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <i class="{{contact.iconAddress}}"></i>
                                <div class="info">
                                    <div class="head">
                                        {{contact.titleAddress}}
                                    </div>
                                    <div class="sub-title">
                                        {{contact.descAddress}}
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <i class="{{contact.iconEmail}}"></i>
                                <div class="info">
                                    <div class="head">
                                        {{contact.titleEmail}}
                                    </div>
                                    <div class="sub-title">
                                        {{contact.descEmail}}
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="column right">
                        <div class="text">
                            {{contact.titleForm}}
                        </div>
                        
                        {{>_msg}}

                        <form action="/contato" method="post">
                            <div class="fields">
                                <div class="field name">
                                    <input type="text" name="name" id="name" placeholder="Digite o nome" value="{{data.name}}" required>
                                </div>
                                <div class="field email">
                                    <input type="email" name="email" id="email" placeholder="Digite o e-mail" value="{{data.email}}"required>
                                </div>
                            </div>
                          
                            <div class="field">
                                <input type="text" name="subject" id="subject" placeholder="Digite o assunto" value="{{data.subject}}" required>
                            </div>

                            <div class="field textarea">
                                <textarea name="content" id="content" cols="30" rows="10" placeholder="Digite o contÃ©udo" required>{{data.content}}</textarea>
                            </div>

                            <div class="button-area">
                                <button type="submit">Enviar</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>